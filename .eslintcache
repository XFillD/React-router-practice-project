\"../hooks/use-http\";\nimport { getSingleQuote } from \"../lib/api\";\nimport { useEffect } from \"react\";\nimport LoadingSpinner from \"../UI/LoadingSpinner\";\n\nconst DUMMYQ = [\n  {\n    id: \"q1\",\n    author: \"Filo\",\n    text: \"jjjjj\",\n  },\n  {\n    id: \"q2\",\n    author: \"FilaDaKila\",\n    text: \"kkkkkk\",\n  },\n];\n\nconst QuoteDetail = () => {\n  const params = useParams();\n  const match = useRouteMatch();\n\n  const { quoteId } = params;\n\n  const { sendRequest, status, data: loadedQuote, error } = useHttp(\n    getSingleQuote,\n    true\n  );\n\n  useEffect(() => {\n    sendRequest(quoteId);\n  }, [sendRequest, quoteId]);\n\n  if (error) {\n    return <p className=\"centered focused\">{error}</p>;\n  }\n\n  if (status === \"pending\") {\n    return (\n      <div className=\"centered\">\n        <LoadingSpinner />\n      </div>\n    );\n  }\n\n  if (!loadedQuote.text) {\n    return <p>No quote found</p>;\n  }\n\n  return (\n    <div>\n      <HighlightedQuote text={loadedQuote.text} author={loadedQuote.author} />\n      <Route path={match.path} exact>\n        <div className=\"centered\">\n          <Link to={`${match.url}/comments`}>\n            <button className=\"btn--flat\">Show Comments</button>\n          </Link>\n        </div>\n      </Route>\n      <Route path={`${match.path}/comments`} exact>\n        <div className=\"centered\">\n          <Link to={`${match.url}`}>\n            <button className=\"btn--flat\">Hide Comments</button>\n          </Link>\n        </div>\n      </Route>\n\n      <Route path={`${match.url}/comments`}>\n        <Comments />\n      </Route>\n    </div>\n  );\n};\nexport default QuoteDetail;\n","/Users/xfilld/Desktop/React-Projects/Router/09-time-to-practice-starting-code/src/components/comments/Comments.js",["101"],"/Users/xfilld/Desktop/React-Projects/Router/09-time-to-practice-starting-code/src/components/comments/NewCommentForm.js",[],"/Users/xfilld/Desktop/React-Projects/Router/09-time-to-practice